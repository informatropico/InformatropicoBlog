  },
  "files": [
    "lib",
    "package.json"
  ],
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "chai": "^3.5.0",
    "eslint": "^0.23.0",
    "metalsmith": "^1.7.0",
    "mocha": "^2.2.5"
  }
